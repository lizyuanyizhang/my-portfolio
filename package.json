{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=3000 --host 127.0.0.1",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "sync:notion": "elog sync -e .elog.env -c elog.config.cjs",
    "sync:essays": "node scripts/notion-to-essays.cjs",
    "sync": "npm run sync:notion && npm run sync:essays && npm run sync:notion:photos && npm run sync:photos && npm run sync:timeline && npm run sync:resume",
    "sync:timeline": "node scripts/notion-to-timeline.cjs",
    "sync:resume": "node scripts/notion-to-resume.cjs",
    "sync:notion:photos": "elog sync -e .elog-photos.env -c elog-photos.config.cjs -a elog-photos.cache.json",
    "sync:photos": "node scripts/notion-to-photos.cjs"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@fingerprintjs/fingerprintjs": "^5.0.1",
    "@google/genai": "^1.29.0",
    "@notionhq/client": "^5.9.0",
    "@supabase/supabase-js": "^2.97.0",
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "@volcengine/openapi": "^1.35.0",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.13.0",
    "tailwind-merge": "^3.5.0",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@elog/cli": "^0.14.7",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.21",
    "supabase": "^2.76.11",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
